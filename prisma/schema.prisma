// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Class {
  id              Int          @id @default(autoincrement())
  price           Int
  duration        Float
  session         Int
  transmission    String
  vehicleType     String
}